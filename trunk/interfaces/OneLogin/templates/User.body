          </div><!-- "jumbotron" -->


        </div><!--/.col-xs-12.col-sm-9-->

        <div class="col-xs-6 col-sm-3 sidebar-offcanvas" id="sidebar" role="navigation">
          <div class="list-group">
            <a href="#" class="list-group-item active">Login settings for {{gcLogin}}</a>
		<form method=post action={{cCGI}} style="margin:0px;">
		<input type=hidden name="gcPage" value="User">
		<input type=hidden name="gcCtHostname" value="{{gcCtHostname}}">
		<input type=hidden name="guContainer" value="{{guContainer}}">
<table border="0" cellspacing="5" cellpadding="0" width="100%" class="table_content">
        <tr>
                <td>
                        <a class=inputLink href="#" onClick="open_popup('{{iCGI}}?gcPage=Glossary&cLabel=Login')"><strong>Login</strong></a>
                </td>
                <td>
                        <input type=text name=cLogin value='{{gcLogin}}' size=40 maxlength=99 disabled class="type_fields_off">
                        <input type=hidden name=cLogin value='{{gcLogin}}'>
                </td>
        </tr>

        <tr>
                <td>
                        <a class=inputLink href="#" onClick="open_popup('{{iCGI}}?gcPage=Glossary&cLabel=Password')"><strong>Current Password</strong></a>
                </td>
                <td>
                        <input type=password name=cCurPasswd autocomplete=off size=40 maxlength=32 class="type_fields">
                </td>
        </tr>

        <tr>
                <td>
                        <a class=inputLink href="#" onClick="open_popup('{{iCGI}}?gcPage=Glossary&cLabel=Password')"><strong>Password</strong></a>
                </td>
                <td>
                        <input type=password name=cPasswd value='{{cPasswd}}' size=40 maxlength=32 class="type_fields">
                </td>
        </tr>

        <tr>
                <td>
                        <a class=inputLink href="#" onClick="open_popup('{{iCGI}}?gcPage=Glossary&cLabel=Password')"><strong>Password Again</strong></a>
                </td>
                <td>
                        <input type=password name=cPasswd2 value='{{cPasswd2}}' size=40 maxlength=32 class="type_fields">
                </td>
        </tr>

        <tr>
                <td> </td>
                <td colspan=2>
                        <input type=submit title='Change your password' class=largeButton name=gcFunction value='Change Password'>
                </td>
        </tr>
</table>		
</form>


	  </div> 
	</div>
        </div><!--/.sidebar-offcanvas-->
            <a href="#" class="list-group-item">Mod PBX</a>
