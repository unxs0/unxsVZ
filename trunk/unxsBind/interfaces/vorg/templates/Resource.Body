			{
				nextPage = newLoc.options[newLoc.selectedIndex].value
			
				if (nextPage != "---")
					document.location.href = "{{cCGI}}?gcPage=Resource"+nextPage
			}
			//--></script>
			{{funcSelectRRType}}
		</td>
	</tr>
	<tr>
	
