<script language="JavaScript" type="text/JavaScript">
	function changePage(newLoc)
	{
		nextPage = newLoc.options[newLoc.selectedIndex].value
				
		if (nextPage != "---")
			document.location.href = "{{cCGI}}?gcPage=Zone&cZone="+nextPage
	}
</script>

